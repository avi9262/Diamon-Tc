        function srcp() {
            // Retrieve selected payment method
            let paymentMethod = getSelectedPaymentMethod();

            // Check if a payment method is selected
            if (paymentMethod) {
                let amount = parseFloat(document.getElementById("to_ammount").value);

                // Check if the amount is valid for the selected payment method
                if (isValidAmount(amount, paymentMethod)) {
                    let link = getPaymentLink(paymentMethod); // Get the base payment URL for the selected method
                    let payurl = link + "?am=" + amount + "&phone=" + phoneUser; // Concatenate phoneUser to the URL
                    window.location.href = payurl; // Redirect to the payment URL
                } else {
                    showErrorMessage("Invalid amount for the selected payment method");
                }
            } else {
                showErrorMessage("Please select a payment method");
            }
        }

        // Function to get the selected payment method
        function getSelectedPaymentMethod() {
            // Retrieve the selected payment method
            let paymentMethod = "";
            if (document.getElementById("bonc").checked) {
                paymentMethod = "FAST-PAY";
            } else if (document.getElementById("bonr").checked) {
                paymentMethod = "E-PAY";
            } else if (document.getElementById("bona").checked) {
                paymentMethod = "RIGHT-PAY";
            } else if (document.getElementById("bonb").checked) {
                paymentMethod = "TB-PAY";
            } else if (document.getElementById("bond").checked) {
                paymentMethod = "EKPAY";
            }
            return paymentMethod;
        }

        // Function to check if the amount is valid for the selected payment method
        function isValidAmount(amount, paymentMethod) {
            // Add logic to check if the amount is valid for the selected payment method
            switch (paymentMethod) {
                case "FAST-PAY":
                    return amount > 299;
                case "E-PAY":
                    return amount > 199;
                case "RIGHT-PAY":
                    return amount > 199;
                case "TB-PAY":
                    return amount > 9;
                case "EKPAY":
                    return amount > 499;
                default:
                    return false;
            }
        }

        // Function to get the base payment URL for the selected method
        function getPaymentLink(paymentMethod) {
            let link = "";
            switch (paymentMethod) {
                case "FAST-PAY":
                    link = "/FAST-PAY";
                    break;
                case "E-PAY":
                    link = "/E-PAY";
                    break;
                case "RIGHT-PAY":
                    link = "/RIGHT-PAY";
                    break;
                case "TB-PAY":
                    link = "/TB-PAY";
                    break;
                case "EKPAY":
                    link = "/EKPAY";
                    break;
                default:
                    break;
            }
            return link;
        }

        // Function to display error message
        function showErrorMessage(message) {
            var x = document.getElementById("copied");
            x.innerHTML = message;
            x.className = "show";
            setTimeout(function () {
                x.className = x.className.replace("show", "");
            }, 3000);
        }


    </script>

